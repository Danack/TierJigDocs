{
    "name": "danack/tierjig",

    "description": "The website for both Tier and Jig.",

    "require-dev": {
        "mockery/mockery": "0.9.1",
        "phpunit/phpunit": "4.4.5"
    },

    "require":{
        "danack/console": "^2.6.1",
        "danack/filefilter": "^0.0.3",
        "danack/jig": "^0.20.0",
        "danack/scripthelper": "^0.0.2",
        "danack/tier": "^0.2.6",
        "intahwebz/configurator": "^0.2.9",
        "leafo/lessphp": "0.4.0",
        "leafo/scssphp": "0.1.1",
        "nikic/fast-route": "0.3.0",
        "oyejorge/less.php": "v1.7.0.1",
        "rdlowrey/auryn": "^1.0.2",
        "room11/caching": "^0.0.4", 
        "room11/http": "^0.0.5",
        "squizlabs/php_codesniffer": "^2.0.0",
        "fluentdom/fluentdom": "~5.3"
    },
    "autoload": {
        "psr-0": {
            "JigDocs": "lib/",
            "Site": "lib/",
            "TierDocs": "lib/",
            "Jig/CompiledTemplate": "var/compilejig/",
            "FileFilter": ["./lib/" ],
